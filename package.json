{
  "name": "@salesforce/plugin-community",
  "description": "Community commands to create and publish an Experience Cloud site, and view a list of available templates in you org.",
  "version": "2.1.4",
  "author": "Salesforce",
  "bugs": "https://github.com/forcedotcom/cli/issues",
  "dependencies": {
    "@oclif/core": "^1.23.0",
    "@oclif/parser": "^3.8.9",
    "@salesforce/core": "^3.33.0",
    "@salesforce/kit": "^1.8.0",
    "@salesforce/sf-plugins-core": "^1.21.5",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^3.2.16",
    "@salesforce/cli-plugins-testkit": "^3.2.15",
    "@salesforce/dev-config": "^3.0.1",
    "@salesforce/dev-scripts": "^3.1.0",
    "@salesforce/plugin-command-reference": "^2.2.8",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "1.4.2",
    "@salesforce/ts-types": "^1.7.2",
    "@swc/core": "^1.3.27",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "chai": "^4.3.7",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-salesforce": "^1.1.0",
    "eslint-config-salesforce-license": "^0.2.0",
    "eslint-config-salesforce-typescript": "^1.1.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsdoc": "^39.6.9",
    "eslint-plugin-sf-plugin": "^1.6.2",
    "husky": "^7.0.4",
    "jsforce": "^2.0.0-beta.18",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "oclif": "^3.4.3",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "shx": "0.3.4",
    "sinon": "^11.1.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.9.4"
  },
  "config": {},
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/salesforcecli/plugin-community",
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference"
    ],
    "topics": {
      "community": {
        "description": "create and publish an Experience Cloud site",
        "longDescription": "Use the community commands to create and publish an Experience Cloud site, and view a list of available templates in you org.",
        "subtopics": {
          "list": {
            "description": "list components in a community"
          }
        }
      }
    }
  },
  "repository": "salesforcecli/plugin-community",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint && yarn test:deprecation-policy && yarn test:command-reference && yarn test:json-schema",
    "prepack": "sf-prepack",
    "prepare": "sf-install",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:command-reference": "./bin/dev commandreference:generate --erroronwarnings",
    "test:deprecation-policy": "./bin/dev snapshot:compare",
    "test:json-schema": "./bin/dev schema:compare",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 600000",
    "version": "oclif-dev readme"
  },
  "publishConfig": {
    "access": "public"
  }
}